<svg width="128" height="128" viewBox="0 0 128 128" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1a1b26;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2d2e3a;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="terminal" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#7aa2f7;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#9ece6a;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="128" height="128" rx="20" fill="url(#bg)"/>
  
  <!-- Terminal window -->
  <rect x="16" y="20" width="96" height="88" rx="8" fill="#1a1b26" stroke="#565f89" stroke-width="2"/>
  
  <!-- Terminal header -->
  <rect x="16" y="20" width="96" height="24" rx="8" fill="#2d2e3a"/>
  <circle cx="28" cy="32" r="4" fill="#f7768e"/>
  <circle cx="40" cy="32" r="4" fill="#e0af68"/>
  <circle cx="52" cy="32" r="4" fill="#9ece6a"/>
  
  <!-- Terminal content -->
  <rect x="20" y="48" width="88" height="52" rx="4" fill="#1a1b26"/>
  
  <!-- Terminal prompt -->
  <text x="24" y="68" font-family="monospace" font-size="12" fill="#7aa2f7">$</text>
  <text x="32" y="68" font-family="monospace" font-size="12" fill="#a9b1d6">hover</text>
  
  <!-- Terminal output -->
  <text x="24" y="84" font-family="monospace" font-size="10" fill="#9ece6a">AI Terminal</text>
  <text x="24" y="96" font-family="monospace" font-size="10" fill="#ff9e64">Ready</text>
  
  <!-- AI sparkle -->
  <circle cx="100" cy="40" r="3" fill="#ff9e64" opacity="0.8"/>
  <circle cx="108" cy="36" r="2" fill="#7aa2f7" opacity="0.6"/>
  <circle cx="104" cy="48" r="2" fill="#9ece6a" opacity="0.7"/>
</svg>